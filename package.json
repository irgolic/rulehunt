  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "^22.8.7",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "wrangler": "^3.75.0",
    "zod": "^4.1.12"
  }
}
